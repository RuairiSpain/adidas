    "grpc": "^1.24.10",
    "ioredis": "^4.27.6",
    "nestjs-log4js": "^0.1.1",
    "opentelemetry-instrumentation-aws-sdk": "^0.5.0",
    "opentelemetry-instrumentation-kafkajs": "^0.5.0",
    "opentelemetry-instrumentation-sequelize": "^0.5.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sequelize": "^6.6.2",
    "sequelize-transparent-cache": "^2.3.0",
    "sequelize-transparent-cache-ioredis": "^2.3.0",
    "sequelize-typescript": "^2.1.0"
