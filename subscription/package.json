    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.17",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/sequelize": "^0.2.0",
    "@opentelemetry/api": "^0.20.0",
    "@opentelemetry/exporter-jaeger": "^0.20.0",
    "@opentelemetry/instrumentation": "^0.20.0",
    "@opentelemetry/instrumentation-dns": "^0.20.0",
    "@opentelemetry/instrumentation-express": "^0.20.0",
    "@opentelemetry/instrumentation-http": "^0.20.0",
    "@opentelemetry/instrumentation-pg": "^0.20.0",
